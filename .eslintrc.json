{
    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true
    },
    "globals": {
        "import.meta": "readonly"
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        // Add any custom rules here
    }
}
